{
  "name": "@julien.cousineau/util",
  "version": "1.0.15",
  "description": "Utility functions for Javascript",
  "main": "build/util.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "eslint --fix src/index.js test/test.js && node test/test.js | faucet",
    "build": "browserify src/index.js -s util -o build/util.js",
    "build-min": "browserify src/index.js -s util | uglifyjs -c warnings=false -m > build/util.min.js",
    "prepare": "npm run build && npm run build-min",
    "push": "git add . && git commit -m 'comment here' && git push -u origin master",
    "patch": "npm version patch && npm publish --access=public",
    "minor": "npm version minor && npm publish --access=public",
    "major": "npm version major && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-Cousineau/util.git"
  },
  "keywords": [
    "utility"
  ],
  "author": "Julien Cousineau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Julien-Cousineau/util/issues"
  },
  "homepage": "https://github.com/Julien-Cousineau/util#readme",
  "devDependencies": {
    "browserify": "^16.2.2",
    "eslint": "^5.0.1",
    "faucet": "0.0.1",
    "istanbul": "^0.4.5",
    "tape": "^4.9.1",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.4.3"
  },
  "dependencies": {}
}
